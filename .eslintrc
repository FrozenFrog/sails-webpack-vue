{
  "env": {
      "node": true,
      "browser": false
  },
  "parserOptions": {
      "ecmaVersion": 7,
      "sourceType": "module",
      "ecmaFeatures": {
          "jsx": false
      }
  },
  "globals": {
      "_": true,
      "sails": true,
      "async": true,
      "server": true,

      "User": true,

      "UserService": true
  },
  "extends":[
      "eslint:recommended",
      "airbnb-base",
      "plugin:node/recommended"
  ],
  "rules":{
      "no-unused-vars": "warn",
      "no-console": "warn"
  }
}
